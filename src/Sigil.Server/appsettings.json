{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BatchWorkers": {
    "EventIngestion": {
      "BatchSize": 200,
      "Cap": 5000,
      "FlushTimeout": "00:00:05"
    },
    "DigestionWorker": {
      "BatchSize": 500,
      "FlushTimeout": "00:01:00"
    }
  },
  "AllowedHosts": "*"
}
